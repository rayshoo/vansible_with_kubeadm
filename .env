FILE_CREATE=yes
MIRROR_CHANGE=yes  # If this is set to yes when the image is centos, the repo file in files/mirror/<version> will be replaced if it exists.

CNI_PLUGIN=calico  # calico, flannel, canal available
METALLB_IP_RANGE=["192.168.125.55-192.168.125.59"]

GROUP_NAME=v8s
MASTER=1
WORKER=3
MASTER_INITIAL=m
MASTER_INITIAL_NUMBER=1
WORKER_INITIAL=w
WORKER_INITIAL_NUMBER=1

# Default Settings
DEFAULT_IMAGE=generic/ubuntu2004  # https://app.vagrantup.com/boxes/search
DEFAULT_MASTER_CPUS=2
DEFAULT_WORKER_CPUS=1
DEFAULT_MASTER_MEMORY=2048
DEFAULT_WORKER_MEMORY=1024
DEFAULT_NETWORK_TYPE=public  # If you change this to public, you should check the network interface name and type it correctly below
DEFAULT_NETWORK_INTERFACE=eno1  # only requires when network type is public
DEFAULT_NETWORK_NETMASK=255.255.128.0
DEFAULT_NETWORK_IP=192.168.125.51
DEFAULT_HOST_PORT=5701
DEFAULT_GUI=no
DEFAULT_RUNTIME=cri-o  # containerd, docker, cri-o available
DEFAULT_SYSTEMD_CGROUP=yes
DEFAULT_SYSTEMD_RESOLVED=yes

# Manual settings override default settings
MANUAL_NETWORK_TYPE=
MANUAL_NETWORK_INTERFACE=
MANUAL_NETWORK_NETMASK=

1_SERVER_IMAGE=
1_SERVER_NAME=
1_SERVER_CPUS=
1_SERVER_MEMORY=
1_SERVER_NETWORK_IP=
1_SERVER_HOST_PORT=
1_SERVER_GUI=
1_SERVER_RUNTIME=
1_SERVER_SYSTEMD_CGROUP=
1_SERVER_SYSTEMD_RESOLVED=

2_SERVER_SYSTEMD_CGROUP=no
2_SERVER_SYSTEMD_RESOLVED=no

3_SERVER_RUNTIME=containerd
3_SERVER_SYSTEMD_CGROUP=yes
3_SERVER_SYSTEMD_RESOLVED=no

4_SERVER_RUNTIME=docker
4_SERVER_SYSTEMD_CGROUP=no
4_SERVER_SYSTEMD_RESOLVED=yes